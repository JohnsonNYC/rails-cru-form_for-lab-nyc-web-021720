<h1>Artists#new</h1>
<p>Find me in app/views/artists/new.html.erb</p>


<%= form_for @artist do |f| %>
    <label><%= f.label 'Name of Artist'%></label>
    <%= f.text_field :name %><br>
    
    <label><%= f.label 'Bio of Artist'%></label>
    <%= f.text_field :bio %><br>

    <%= f.submit 'Create Artist' %>

<% end %>